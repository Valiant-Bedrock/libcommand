  "type": "library",
  "version": "0.6.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "libcommand\\": "src/libcommand"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "1.6.x-dev",
