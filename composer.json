{
  "name": "valiant-bedrock/libcommand",
  "description": "A PocketMine-MP library for registering commands with proper overloads",
  "type": "library",
  "version": "0.1.0",
  "autoload": {
    "psr-4": {
      "libcommand\\": "src/libcommand"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "1.6.x-dev",
    "pocketmine/pocketmine-mp": "dev-stable"
  },
  "scripts": {
    "analyze": "./vendor/bin/phpstan analyze"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "sylvrs",
      "email": "personal@matthewjordan.xyz"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/PocketMine-MP.git" },
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/BedrockProtocol.git" },
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/BedrockData.git" }
  ]
}
