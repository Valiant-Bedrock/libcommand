{
  "name": "valiant-bedrock/libcommand",
  "description": "A PocketMine-MP library for registering commands with proper overloads",
  "type": "library",
  "version": "0.7.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "libcommand\\": "src/libcommand"
    }
  },
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10.15",
    "friendsofphp/php-cs-fixer": "^v3.17.0",
    "pocketmine/pocketmine-mp": "dev-stable"
  },
  "scripts": {
    "analyze": "./vendor/bin/phpstan analyze",
    "run-cs-fixer": "./vendor/bin/php-cs-fixer fix --diff --ansi"
  },
  "authors": [
    {
      "name": "sylvrs",
      "email": "personal@matthewjordan.xyz"
    }
  ],
  "minimum-stability": "dev"
}
